      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [
        {
          "name": "PATH",
          "value": "E:\\libtorch-win-shared-with-deps-2.7.0+cu128\\libtorch\\bin;E:\\libtorch-win-shared-with-deps-2.7.0+cu128\\libtorch\\lib;C:\\Program Files\\NVIDIA GPU Computing Toolkit\\CUDA\\v12.8\\bin;${env:PATH}"
